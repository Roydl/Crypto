<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net6.0;net7.0;netcoreapp3.1</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <IsPackable>false</IsPackable>
    <Authors>Roy Schroedel</Authors>
    <Company>Roy Schroedel</Company>
    <Product>Roydl.Crypto.Test</Product>
    <AssemblyName>Roydl.Crypto.Test</AssemblyName>
    <RootNamespace>Roydl.Crypto.Test</RootNamespace>
    <Copyright>© Roy Schroedel. All rights reserved.</Copyright>
    <PackageProjectUrl>https://github.com/Roydl/Crypto</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Roydl/Crypto</RepositoryUrl>
    <NeutralLanguage>en-US</NeutralLanguage>
    <PackageIcon>LOGO.png</PackageIcon>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <StartupObject>AutoGeneratedProgram</StartupObject>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>Roydl.Crypto.Test.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NUnit" Version="3.13.3" />
    <PackageReference Include="NUnit3TestAdapter" Version="4.4.2" />
    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="17.5.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
      <Visible>false</Visible>
    </None>
    <None Include="..\LOGO.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
      <Visible>false</Visible>
    </None>
    <None Include="Roydl.Crypto.Test.snk">
      <Pack>False</Pack>
      <Visible>False</Visible>
    </None>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\src\Roydl.Crypto.csproj" />
  </ItemGroup>

</Project>
